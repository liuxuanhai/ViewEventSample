<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.loften.vieweventsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".customtouch.MultitouchActivity" />
        <activity android:name=".customtouch.TouchDelegateActivity" />
        <activity android:name=".customtouch.TouchForwardActivity" />
        <activity android:name=".customtouch.TouchInterceptActivity" />
        <activity android:name=".customtouch.TouchListenerActivity" />
        <activity android:name=".customtouch.TwoDimensionGestureScrollActivity" />
        <activity android:name=".customtouch.TwoDimensionScrollActivity" />
        <activity android:name=".customtouch.MoveLoggerActivity" />
        <activity android:name=".mysample.ViewEventActivity"></activity>
    </application>

</manifest>